                              <Insets />
                           </VBox.margin>
                        </Button>
                        <Button alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#nextScene" text="LOGIN AS GUEST">
