                  </VBox>
                  <VBox id="password" alignment="CENTER" minHeight="0.0" minWidth="0.0" spacing="5.0">
                     <children>
                        <Label text="Password">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <PasswordField fx:id="userPassword" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
